    <string name="information_page_string_four">Irish Citizen</string>
    <string name="information_page_string_five">Resident in the republic</string>
    <string name="information_page_string_six">Registered to vote</string>
    <string name="information_page_string_seven">Check to see if you are registered</string>
    <string name="information_page_string_eight">checktheregister.ie</string>
    <string name="information_page_string_thirteen">On May 22nd</string>
    <string name="information_page_string_fourteen">Polling stations will be open from 7am to 10pm</string>
